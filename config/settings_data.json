{
  "current": {
    "font_heading": "serif",
    "font_body": "sans-serif",
    "checkout_input_background_color_mode": "transparent",
    "checkout_sidebar_background_color": "#fff",
    "checkout_accent_color": "#dd1d1d",
    "checkout_button_color": "#dd1d1d",
    "content_for_index": [

    ],
    "color_schemes": {
      "default": {
        "settings": {
          "canvas": "#ffffff",
          "ink": "#000000",
          "stroke": "#a3a3a3",
          "canvas_subdued": "#f7f7f7",
          "ink_subdued": "#a3a3a3",
          "primary": "{{ shop.brand.colors.primary[0].background }}",
          "primary_contrasting": "{{ shop.brand.colors.primary[0].foreground }}",
          "secondary": "{{ shop.brand.colors.secondary[0].background }}",
          "secondary_contrasting": "{{ shop.brand.colors.secondary[0].foreground }}"
        }
      },
      "gray": {
        "settings": {
          "canvas": "#f7f7f7",
          "ink": "#000000",
          "stroke": "#a3a3a3",
          "canvas_subdued": "#ebebeb",
          "ink_subdued": "#a3a3a3",
          "primary": "{{ shop.brand.colors.primary[0].background }}",
          "primary_contrasting": "{{ shop.brand.colors.primary[0].foreground }}",
          "secondary": "{{ shop.brand.colors.secondary[0].background }}",
          "secondary_contrasting": "{{ shop.brand.colors.secondary[0].foreground }}"
        }
      },
      "dark": {
        "settings": {
          "canvas": "#000000",
          "ink": "#ffffff",
          "stroke": "#616161",
          "canvas_subdued": "#303030",
          "ink_subdued": "#8a8a8a",
          "primary": "{{ shop.brand.colors.primary[0].background }}",
          "primary_contrasting": "{{ shop.brand.colors.primary[0].foreground }}",
          "secondary": "{{ shop.brand.colors.secondary[0].background }}",
          "secondary_contrasting": "{{ shop.brand.colors.secondary[0].foreground }}"
        }
      }
    }
  }
